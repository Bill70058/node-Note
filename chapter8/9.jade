doctype
html
    head
        title
        style.
            div{
                width:200px;
                height:100px;
            }
            .div1 {background:red}
            .div2 {background:black}
            .div3 {background:green}
            .div4 {background:#ccc}
    body
        -var arr = ['div1', 'div2', 'div3', 'div4']
        -for(var i = 0; i < arr.length; i++)
         div(class=arr[i])